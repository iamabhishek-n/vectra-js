generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Document {
  id         String   @id @default(uuid()) @db.Uuid
  content    String   @db.Text
  metadata   Json?    @db.JsonB
  embedding  Unsupported("vector")
  createdAt  DateTime @default(now()) @db.Timestamptz(6)

  @@map("Document")
}
